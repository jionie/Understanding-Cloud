ninja_required_version = 1.3
cxx = c++

cflags = -DTORCH_EXTENSION_NAME=enclib_cpu -DTORCH_API_INCLUDE_EXTENSION_H -isystem /opt/anaconda3/lib/python3.7/site-packages/torch/include -isystem /opt/anaconda3/lib/python3.7/site-packages/torch/include/torch/csrc/api/include -isystem /opt/anaconda3/lib/python3.7/site-packages/torch/include/TH -isystem /opt/anaconda3/lib/python3.7/site-packages/torch/include/THC -isystem /opt/anaconda3/include/python3.7m -D_GLIBCXX_USE_CXX11_ABI=0 -fPIC -std=c++11
ldflags = -shared

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out
  depfile = $out.d
  deps = gcc

rule link
  command = $cxx $in $ldflags -o $out

build operator.o: compile /root/share/project/kaggle/2019/steel/code/dummy_semi_14/lib/net/tex_encode/src/cpu/operator.cpp
build encoding_cpu.o: compile /root/share/project/kaggle/2019/steel/code/dummy_semi_14/lib/net/tex_encode/src/cpu/encoding_cpu.cpp
build syncbn_cpu.o: compile /root/share/project/kaggle/2019/steel/code/dummy_semi_14/lib/net/tex_encode/src/cpu/syncbn_cpu.cpp
build roi_align_cpu.o: compile /root/share/project/kaggle/2019/steel/code/dummy_semi_14/lib/net/tex_encode/src/cpu/roi_align_cpu.cpp
build nms_cpu.o: compile /root/share/project/kaggle/2019/steel/code/dummy_semi_14/lib/net/tex_encode/src/cpu/nms_cpu.cpp

build enclib_cpu.so: link operator.o encoding_cpu.o syncbn_cpu.o roi_align_cpu.o nms_cpu.o

default enclib_cpu.so

